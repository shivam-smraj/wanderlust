<!-- <h1>Falsh mess</h1> -->


<% if(success && success.length){ %>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3 "  style="text-align: center;"  role="alert">
         <!-- <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Warning:"><use xlink:href="#exclamation-triangle-fill"/></svg> -->
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>


<% if(error && error.length ){ %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 " style="text-align: center;" role="alert">
         <!-- <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Warning:"><use xlink:href="#exclamation-triangle-fill"/></svg> -->
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>
